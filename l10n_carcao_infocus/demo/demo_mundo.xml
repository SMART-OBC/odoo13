<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="partner_demo_company_infocus" model="res.partner">
        <field name="name">Infocus Company</field>
        <field name="vat">J770023598</field>
        <field name="street">a</field>
        <field name="city">Maracaibo</field>
        <field name="country_id" ref="base.cw"/>

        <field name="zip">4032</field>
        <field name="phone">+58 412-1234567</field>
        <field name="email">info@company.veexample.com</field>

        <field name="website">www.cwexample.com</field>
    </record>

    <record id="demo_company_infocus" model="res.company">
        <field name="name">Infocus Company</field>
<!--        <field name="po_lead">1.0</field>-->
        <field name="partner_id" ref="partner_demo_company_infocus"/>
    </record>

    <function model="res.company" name="_onchange_country_id">
        <value eval="[ref('demo_company_infocus')]"/>
    </function>

    <function model="res.users" name="write">
        <value eval="[ref('base.user_root'), ref('base.user_admin'), ref('base.user_demo')]"/>
        <value eval="{'company_ids': [(4, ref('l10n_carcao_infocus.demo_company_infocus'))]}"/>
    </function>

    <function model="account.chart.template" name="try_loading">
        <value eval="[ref('l10n_carcao_infocus.cw_chart_template_infocus')]"/>
        <value model="res.company" eval="obj().env.ref('l10n_carcao_infocus.demo_company_infocus')"/>
    </function>
</odoo>
